<%= stylesheet_link_tag 'users' %>

<h1>Account Login</h1>

<%= form_tag("/users/logincheck", method: "post") do %>
  <label>Email:</label>
  <%= email_field_tag :email %>

  <label>Password:</label>
  <%= password_field_tag :password %>


  <%= submit_tag "Submit" %>
<% end %>

<%= link_to "New User", "/users/new", class: "new-user-link" %>